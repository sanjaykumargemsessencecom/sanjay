<div class='form_div'>
    <%= form_tag session_path do %>
   <div id='f'>
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email] %><br>
   </div>
     <div id='f'>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %><br>
    </div>
   
      <%= submit_tag "Sign in", class: "b" %>

<% end %>
